rt',\n    props: {\n      users: Array,\n    },\n    data() {\n      return {\n        chart: null,\n        fakeData: [\n          { name: 'User 1', logins: 7 },\n          { name: 'User 2', logins: 5 },\n        ],\n      };\n    },\n    mounted() {\n      this.userUpdate();\n      this.initChart();\n    },\n    methods: {\n      async userUpdate(){\n        await new Promise(resolve => setTimeout(resolve, 1000));\n        const firebaseApp = initializeApp(firebaseConfig);\n        const db = getFirestore(firebaseApp);\n        const loginsCollection = collection(db, 'logins');\n              const snapshot = await getDocs(loginsCollection);\n\n      // Process each document in the snapshot\n      const users = [];\n      snapshot.forEach((doc) => {\n        const data = doc.data();\n        console.log(data.username)\n        const user = {\n          id: 1,\n          name: data.username, \n          lastLogin: this.formatTimestamp(data.time), \n        };\n        users.push(user);\n      });\n\n      // Update the users with the result\n      this.users = users.length;\n\n      },\n      initChart() {\n        const ctx = this.$refs.chart.getContext('2d');\n        this.chart = new Chart(ctx, {\n          type: 'bar',\n          data: {\n            labels: this.fakeData.map(user => user.name),\n            datasets: [{\n              label: 'Number of Logins',\n              data: this.fakeData.map(user => user.logins),\n              backgroundColor: [\n                'rgba(255, 99, 132, 0.2)',\n                'rgba(54, 162, 235, 0.2)',\n                'rgba(255, 206, 86, 0.2)',\n              ],\n              borderColor: [\n                'rgba(255, 99, 132, 1)',\n                'rgba(54, 162, 235, 1)',\n                'rgba(255, 206, 86, 1)',\n              ],\n              borderWidth: 1\n            }]\n          },\n          options: {\n            scales: {\n              y: {\n                beginAtZero: true\n              }\n            },\n            responsive: false,\n          }\n        });\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n    canvas {\n        width: 100%;\n        height: 100%;\n        margin: auto;\n    }\n    .login-chart {\n        width: 100%;\n        /* max-width: 600px; */\n        margin: 20px auto;\n    }\n  </style>\n  "],"mappings":";AAOE,OAAOA,KAAI,MAAO,eAAe;AACjC,SAASC,aAAY,QAAS,cAAc;AAC5C,SAASC,YAAY,EAAEC,UAAU,EAAEC,OAAM,QAAS,oBAAoB;AAEtE,MAAMC,cAAa,GAAI;EACrBC,MAAM,EAAE,yCAAyC;EACjDC,UAAU,EAAE,gCAAgC;EAC5CC,WAAW,EAAE,uCAAuC;EACpDC,SAAS,EAAE,gBAAgB;EAC3BC,aAAa,EAAE,4BAA4B;EAC3CC,iBAAiB,EAAE,cAAc;EACjCC,KAAK,EAAE;AACT,CAAC;AAED,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,KAAK,EAAEC;EACT,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,QAAQ,EAAE,CACR;QAAEN,IAAI,EAAE,QAAQ;QAAEO,MAAM,EAAE;MAAE,CAAC,EAC7B;QAAEP,IAAI,EAAE,QAAQ;QAAEO,MAAM,EAAE;MAAE,CAAC;IAEjC,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMF,UAAUA,CAAA,EAAE;MAChB,MAAM,IAAIG,OAAO,CAACC,OAAM,IAAKC,UAAU,CAACD,OAAO,EAAE,IAAI,CAAC,CAAC;MACvD,MAAME,WAAU,GAAI3B,aAAa,CAACI,cAAc,CAAC;MACjD,MAAMwB,EAAC,GAAI3B,YAAY,CAAC0B,WAAW,CAAC;MACpC,MAAME,gBAAe,GAAI3B,UAAU,CAAC0B,EAAE,EAAE,QAAQ,CAAC;MAC3C,MAAME,QAAO,GAAI,MAAM3B,OAAO,CAAC0B,gBAAgB,CAAC;;MAExD;MACA,MAAMf,KAAI,GAAI,EAAE;MAChBgB,QAAQ,CAACC,OAAO,CAAEC,GAAG,IAAK;QACxB,MAAMhB,IAAG,GAAIgB,GAAG,CAAChB,IAAI,CAAC,CAAC;QACvBiB,OAAO,CAACC,GAAG,CAAClB,IAAI,CAACmB,QAAQ;QACzB,MAAMC,IAAG,GAAI;UACXC,EAAE,EAAE,CAAC;UACLzB,IAAI,EAAEI,IAAI,CAACmB,QAAQ;UACnBG,SAAS,EAAE,IAAI,CAACC,eAAe,CAACvB,IAAI,CAACwB,IAAI;QAC3C,CAAC;QACD1B,KAAK,CAAC2B,IAAI,CAACL,IAAI,CAAC;MAClB,CAAC,CAAC;;MAEF;MACA,IAAI,CAACtB,KAAI,GAAIA,KAAK,CAAC4B,MAAM;IAEzB,CAAC;IACDpB,SAASA,CAAA,EAAG;MACV,MAAMqB,GAAE,GAAI,IAAI,CAACC,KAAK,CAAC3B,KAAK,CAAC4B,UAAU,CAAC,IAAI,CAAC;MAC7C,IAAI,CAAC5B,KAAI,GAAI,IAAIlB,KAAK,CAAC4C,GAAG,EAAE;QAC1BG,IAAI,EAAE,KAAK;QACX9B,IAAI,EAAE;UACJ+B,MAAM,EAAE,IAAI,CAAC7B,QAAQ,CAA